# Probar que no exista correcursiÃ³n.
func predfact(number n) -> number
begin
    return factorial(n-1);
end;

func factorial(number n) -> number
begin
    if n==1 or n==0 then
        return 1;
    else
        return (n*(predfact(n)));
    end;   
end;

program
  with
      number i;
      number j = 78;
  do
      i = factorial(4);
        with
          boolean i;
        do
          i = true;
        end;
      j = 78 + i;
  end;
end;