# Probar que no pueda usarse la variable como argumento al declararla asignÃ¡ndole una funciÃ³n.
func plus(number op1, number op2) -> number
begin
    return op1 + op2;
end;
program
  with
      number i = 5;
      number j = 78 + plus(4, 3);
  do
      i = plus(3,2);
        with
          number i = plus(2,i);
        do
          i = true;
        end;
      j = 78 + i;
  end;
end;